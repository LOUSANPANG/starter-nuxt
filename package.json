{
  "name": "starter-nuxt",
  "type": "module",
  "scripts": {
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "nuxt dev --envName development",
    "test": "nuxt dev --envName test",
    "build:test": "nuxt build --envName test",
    "preview:test": "nuxt preview --envName test",
    "generate:test": "nuxt generate --envName test",
    "build": "nuxt build --envName production",
    "preview": "nuxt preview --envName production",
    "generate": "nuxt generate --envName production"
  },
  "dependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/image": "1.10.0",
    "@pinia/nuxt": "^0.11.0",
    "@tailwindcss/vite": "^4.1.4",
    "eslint": "^9.25.1",
    "install": "^0.13.0",
    "nuxt": "^3.17.0",
    "pinia": "^3.0.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "sass": "^1.87.0"
  }
}
